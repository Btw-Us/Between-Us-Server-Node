# PocketBase Configuration
# URL where PocketBase is running (use http://127.0.0.1:8090 when using Docker)
POCKETBASE_URL=http://localhost:8090

# PocketBase Admin Credentials (CHANGE IN PRODUCTION!)
POCKETBASE_ADMIN_EMAIL=admin@example.com
POCKETBASE_ADMIN_PASSWORD=your_secure_password_here

# Server Configuration
PORT=3000
SERVER_JWT_SECRET=your_jwt_secret_key